<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="248.0" prefWidth="353.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HT2.ControllerHT">
   <top>
      <Label text="Gym Observer" underline="true" BorderPane.alignment="BOTTOM_CENTER">
         <BorderPane.margin>
            <Insets top="50.0" />
         </BorderPane.margin>
         <font>
            <Font name="Bodoni MT" size="32.0" />
         </font>
      </Label>
   </top>
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <top>
            <Label text="Versio 1.0" BorderPane.alignment="BOTTOM_CENTER">
               <BorderPane.margin>
                  <Insets top="25.0" />
               </BorderPane.margin>
            </Label>
         </top>
         <center>
            <Label text="Joel Helkala" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="15.0" />
               </BorderPane.margin>
            </Label>
         </center>
      </BorderPane>
   </center>
   <bottom>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <Label minWidth="-Infinity" text="Anna salin nimi">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="textVastaus" alignment="CENTER" maxWidth="-Infinity" prefWidth="1000.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <BorderPane>
               <right>
                  <Button mnemonicParsing="false" onAction="#handleAvausOk" text="OK" BorderPane.alignment="CENTER">
                     <tooltip>
                        <Tooltip text="Avaa salin kohdalta" />
                     </tooltip>
                     <BorderPane.margin>
                        <Insets right="10.0" />
                     </BorderPane.margin>
                  </Button>
               </right>
               <center>
                  <Button fx:id="buttonCancel" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" BorderPane.alignment="TOP_RIGHT">
                     <BorderPane.margin>
                        <Insets right="5.0" />
                     </BorderPane.margin>
                  </Button>
               </center>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </BorderPane>
         </children>
      </VBox>
   </bottom>
</BorderPane>
